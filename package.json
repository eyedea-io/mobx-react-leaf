{
  "name": "mobx-react-leaf",
  "version": "0.1.0",
  "devDependencies": {
    "@kadira/storybook": "^2.5.2",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-polyfill": "^6.13.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "case-sensitive-paths-webpack-plugin": "1.1.3",
    "chalk": "^1.1.3",
    "coercer": "^1.1.2",
    "compression": "^1.6.2",
    "cross-spawn": "4.0.0",
    "css-loader": "0.23.1",
    "enhanced-resolve": "^3.0.3",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "filesize": "3.5.0",
    "fs-extra": "1.0.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "^2.26.0",
    "ip": "^1.1.3",
    "jest": "^18.0.0",
    "json-loader": "0.5.4",
    "minimist": "^1.2.0",
    "object-assign": "4.1.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-focus": "^1.0.0",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.3.1",
    "postcss-nested": "^1.0.0",
    "promise": "7.1.1",
    "react-hot-loader": "^3.0.0-alpha.8",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "beta",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-hot-middleware": "^2.12.2",
    "whatwg-fetch": "2.0.1",
    "xo": "^0.17.1",
    "xo-loader": "^0.7.0",
    "z-schema": "^3.17.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "hoist-non-react-statics": "^1.2.0",
    "mobx": "^3.0.2",
    "mobx-react": "^4.1.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^4.0.0-2"
  },
  "scripts": {
    "start": "node ./scripts/start.js",
    "build": "node ./scripts/build.js",
    "test": "xo && jest --env=jsdom",
    "storybook": "start-storybook -p 6006 --config-dir=./config/storybook",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.(jpg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm)$": "<rootDir>\\config\\jest\\FileStub.js",
      "^[./a-zA-Z0-9$_-]+\\.s?css$": "<rootDir>\\config\\jest\\CSSStub.js"
    },
    "scriptPreprocessor": "<rootDir>\\config\\jest\\transform.js",
    "setupFiles": [
      "<rootDir>\\config\\polyfills.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|node_modules|config|scripts)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": "xo-react",
    "esnext": true,
    "space": true,
    "module": false,
    "semicolon": true,
    "ignores": [
      "config",
      "scripts/build.js",
      "node_modules",
      "\\.(json|css|jpg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm)$"
    ],
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "config/webpack.config.dev.js"
        }
      }
    },
    "envs": [
      "browser",
      "mocha"
    ],
    "rules": {
      "object-curly-spacing": 0,
      "unicorn/filename-case": 0,
      "react/forbid-component-props": 0,
      "react/jsx-no-bind": 0,
      "import/named": 0,
      "import/prefer-default-export": 0,
      "import/no-unresolved": 0,
      "import/no-unassigned-import": 0
    }
  }
}
